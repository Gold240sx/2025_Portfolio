{
  "name": "payload_portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:reset": "drizzle-kit drop && drizzle-kit push:pg && payload migrate",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "payload:build": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.4",
    "@payloadcms/db-postgres": "^3.24.0",
    "@payloadcms/next": "^3.24.0",
    "@payloadcms/richtext-lexical": "^3.24.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.66.9",
    "@trpc/client": "11.0.0-rc.802",
    "@trpc/react-query": "11.0.0-rc.802",
    "@trpc/server": "11.0.0-rc.802",
    "cross-env": "^7.0.3",
    "drizzle-orm": "^0.39.3",
    "escape-html": "^1.0.3",
    "geist": "^1.3.1",
    "graphql": "^16.10.0",
    "next": "^15.1.7",
    "next-auth": "5.0.0-beta.25",
    "payload": "^3.24.0",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "sharp": "^0.33.5",
    "superjson": "^2.2.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/escape-html": "^1.0.4",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.1.7",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.4.35",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "packageManager": "pnpm@9.15.3"
}
